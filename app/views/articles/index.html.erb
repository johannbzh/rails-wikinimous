<div class = "container d-flex justify-content-between border width: 100% " id="banner">
  <h3>Wikinimious</h3>
  <p><%= link_to "New Article", new_article_path(@article) %></p>
</div>
<div class="container">
  <% @articles.each do |article| %>
    <li><strong><%= link_to article.title, article_path(article) %></strong></li>
    <em> <%= article.content %></em>
    <hr>
  <% end %>
</div>
